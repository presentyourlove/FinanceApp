{
  "expo": {
    "name": "FinanceApp",
    "slug": "FinanceApp",
    "owner": "jasonwth",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "financeapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes"
    },
    "android": {
      "jsEngine": "hermes",
      "package": "com.jasonwth.financeapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "459500bc-01b0-452b-a003-cb7cec6d2000"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/459500bc-01b0-452b-a003-cb7cec6d2000"
    }
  }
}